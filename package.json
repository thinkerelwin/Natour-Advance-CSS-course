{
  "name": "natours",
  "version": "1.0.0",
  "description": "RWD css animation webpage",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel scss devserver",
    "compile-scss": "node-sass sass/main.scss css/style.comp.css",
    "concat-css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix-css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress-css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build-css": "npm-run-all compile-scss concat-css prefix-css compress-css"
  },
  "keywords": [
    "RWD",
    "CSS"
  ],
  "author": "Elwin",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "concat": "^1.0.3",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  }
}
